  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {},
  "devDependencies": {
    "@types/tape": "^4.13.0",
    "browserify": "^16.5.1",
    "coveralls": "^3.1.0",
    "nyc": "^15.0.1",
    "tap-nyc": "^1.0.3",
    "tap-spec": "^5.0.0",
    "tape": "^5.0.0",
    "tslint": "^6.1.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
