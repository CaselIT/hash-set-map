  "bugs": {
    "url": "https://github.com/CaselIT/hash-set-map/issues"
  },
  "homepage": "https://github.com/CaselIT/hash-set-map#readme",
  "files": [
    "build/index*",
    "build/src/**/*",
    "LICENSE",
    "README.md"
